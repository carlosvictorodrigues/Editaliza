        "{cmd} -p \"{prompt}\" --model gemini-2.0-pro -y",
        "{cmd} -p \"{prompt}\" --model gemini-1.5-pro -y",
        "{cmd} -p \"{prompt}\" -m gemini-2.5-pro -y",
        "{cmd} -p \"{prompt}\" -m gemini-2.0-pro -y",
        "{cmd} -p \"{prompt}\" -m gemini-1.5-pro -y",
        "echo \"{prompt}\" | {cmd} -p \"\" --model gemini-1.5-pro -y"
      ],
      "timeout_ms": 300000,
      "retry_count": 2
    }
  },
  "monitoring": {
    "health_check_interval": 30000,
