  },
  "deploy_commands": [
    "git add .",
    "git commit -m \"fix: auto-fix by orchestrator\"",
    "git push origin main",
    "ssh editaliza 'cd /root/editaliza && git pull && npm install --production && pm2 restart editaliza-app'"
  ],
  "orchestration": {
    "max_turns": 16,
    "parallel_execution": true,
    "auto_retry": true,
    "stop_tokens": ["<DONE>"],
    "sync_token": "<SYNC_GO>"
  },
  "architect": {
    "command": "claude",
    "patterns": [
      "{cmd} --model claude-3-opus-latest --print --dangerously-skip-permissions --add-dir . \"{prompt}\"",
      "{cmd} --model claude-3-opus-20240229 --print --dangerously-skip-permissions --add-dir . \"{prompt}\"",
      "{cmd} --model opus --print --dangerously-skip-permissions --add-dir . \"{prompt}\"",
      "echo \"{prompt}\" | {cmd} --model claude-3-opus-latest --print --dangerously-skip-permissions --add-dir ."
    ],
    "timeout_ms": 120000,
    "retry_count": 2,
    "env": {
      "NO_COLOR": "1",
      "CI": "1"
    }
  },
  "architect_fallback": {
    "command": "gemini",
    "patterns": [
      "{cmd} -p \"{prompt}\" --model gemini-2.5-pro",
      "{cmd} -p \"{prompt}\" --model gemini-2.0-pro",
      "{cmd} -p \"{prompt}\" --model gemini-1.5-pro",
      "{cmd} -p \"{prompt}\" -m gemini-2.5-pro",
      "{cmd} -p \"{prompt}\" -m gemini-2.0-pro",
      "{cmd} -p \"{prompt}\" -m gemini-1.5-pro",
      "echo \"{prompt}\" | {cmd} -p \"\" --model gemini-1.5-pro"
    ],
    "timeout_ms": 120000,
    "retry_count": 1
  },
  "agents": {
    "fe": {
      "name": "Frontend Agent",
      "command": "gemini",
      "specialization": "Frontend, React, TypeScript, CSS, UX",
      "patterns": [
        "{cmd} -p \"{prompt}\" --model gemini-2.5-pro -y",
        "{cmd} -p \"{prompt}\" -m gemini-2.5-pro -y",
        "echo \"{prompt}\" | {cmd} -p \"\" --model gemini-2.5-pro -y"
      ],
      "timeout_ms": 300000,
      "retry_count": 2
    },
    "be": {
      "name": "Backend Agent",
      "command": "claude",
      "specialization": "Node.js, Express, APIs, Business Logic",
      "patterns": [
        "{cmd} --model claude-3-5-sonnet-latest --print --dangerously-skip-permissions --add-dir . \"{prompt}\"",
        "{cmd} --model claude-3-5-sonnet-20240620 --print --dangerously-skip-permissions --add-dir . \"{prompt}\"",
        "{cmd} --model sonnet --print --dangerously-skip-permissions --add-dir . \"{prompt}\"",
        "echo \"{prompt}\" | {cmd} --model claude-3-5-sonnet-latest --print --dangerously-skip-permissions --add-dir ."
      ],
      "timeout_ms": 300000,
      "retry_count": 3,
      "env": {
        "NO_COLOR": "1",
        "CI": "1"
      }
    },
    "devops": {
      "name": "DevOps Agent",
      "command": "claude",
      "specialization": "Deploy, PM2, Nginx, SSH, CI/CD",
      "patterns": [
        "{cmd} --model claude-3-5-sonnet-latest --print --dangerously-skip-permissions --add-dir . \"{prompt}\"",
        "{cmd} --model claude-3-5-sonnet-20240620 --print --dangerously-skip-permissions --add-dir . \"{prompt}\"",
        "{cmd} --model sonnet --print --dangerously-skip-permissions --add-dir . \"{prompt}\"",
        "echo \"{prompt}\" | {cmd} --model claude-3-5-sonnet-latest --print --dangerously-skip-permissions --add-dir ."
      ],
      "timeout_ms": 300000,
      "retry_count": 3,
      "env": {
        "NO_COLOR": "1",
        "CI": "1"
      }
    },
    "dba": {
      "name": "Database Agent",
      "command": "claude",
      "specialization": "PostgreSQL, Migrations, Indexes, Query Optimization",
      "patterns": [
        "{cmd} --model claude-3-5-sonnet-latest --print --dangerously-skip-permissions --add-dir . \"{prompt}\"",
        "{cmd} --model claude-3-5-sonnet-20240620 --print --dangerously-skip-permissions --add-dir . \"{prompt}\"",
        "{cmd} --model sonnet --print --dangerously-skip-permissions --add-dir . \"{prompt}\"",
        "echo \"{prompt}\" | {cmd} --model claude-3-5-sonnet-latest --print --dangerously-skip-permissions --add-dir ."
      ],
      "timeout_ms": 300000,
      "retry_count": 3,
      "env": {
        "NO_COLOR": "1",
        "CI": "1"
      }
    },
    "qa": {
      "name": "QA Agent",
